<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="view_move_expense_form" model="ir.ui.view">
        <field name="name">view_move_expense_form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='aml_tab']/field[@name='line_ids']/tree/field[@name='partner_id']" position="after">
                <field name="sale_order_id" optional="show" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>