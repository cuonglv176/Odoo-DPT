
<odoo>
    <record id="seq_dpt_shipping_slip" model="ir.sequence">
        <field name="name">DPT Shipping Slip Code</field>
        <field name="code">shipping.slip.seq</field>
        <field name="prefix">KTSS-%(y)s%(month)s-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
