<odoo>
    <record id="view_port_tree" model="ir.ui.view">
        <field name="name">import.port.tree</field>
        <field name="model">dpt.import.port</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>

    <record id="view_port_form" model="ir.ui.view">
        <field name="name">import.port.form</field>
        <field name="model">dpt.import.port</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <field name="name"/>
                    <field name="code"/>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_brand_tree" model="ir.ui.view">
        <field name="name">import.brand.tree</field>
        <field name="model">dpt.import.brand</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_brand_form" model="ir.ui.view">
        <field name="name">import.brand.form</field>
        <field name="model">dpt.import.brand</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <field name="name"/>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_import_port" model="ir.actions.act_window">
        <field name="name">Ports</field>
        <field name="res_model">dpt.import.port</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_import_brand" model="ir.actions.act_window">
        <field name="name">Brands</field>
        <field name="res_model">dpt.import.brand</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
